{
 "manifest_version": 3,
 "name": "Ad Deception Detector",
 "version": "1.0",
 "description": "Detects deceptive buttons that open ads in new tabs.",
 "permissions": ["activeTab"],
 "background": {
   "service_worker": "background.js"
  },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "icons": {
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
  